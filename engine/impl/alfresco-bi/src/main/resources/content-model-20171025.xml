                <property name="edms:fiscalyear">
                    <type>d:int</type>
                    <mandatory>true</mandatory>
                    <multiple>false</multiple>
                    <constraints>
                        <constraint ref="edms:yearconstraint" />
                    </constraints>
                </property>
                <property name="edms:office">
                    <type>d:category</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                </property>
            </properties>
            <mandatory-aspects>
                <aspect>edms:functionaspect</aspect>
