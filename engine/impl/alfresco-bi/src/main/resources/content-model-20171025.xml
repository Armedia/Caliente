                    <value>T01 - Procurement Request</value>
                    <value>T02 - Technical Docs</value>        
                    <value>T03 - Acquisition Planning</value>
                    <value>T04 - Determinations and Findings/Justifications and Approvals/Ratifications</value>
                    <value>T05 - Wage Determinations/Labor Agreements</value>
                    <value>T06 - Small Business Set-Aside Determination</value>        
                    <value>T07 - Source List/Market Research</value>
                    <value>T08 - Publicizing Pre-Award Contract Actions</value>
                    <value>T09 - Pre-Solicitation/Pre-Proposal/Site Visits</value>
                    <value>T10 - Solicitation and Amendments</value>        
                    <value>T11 - Abstract of Bids/Proposals/Offers</value>
                    <value>T12 - Cost of Pricing Data or Waiver</value>
                    <value>T13 - Data to Support Proposal Analysis</value>
                    <value>T14 - Independent Government Estimate (IGE) and Pre-Award Analysis</value>        
                    <value>T15 - Award Decision Documentation</value>
                    <value>T16 - Certificate of Current Cost or Pricing Data</value>
                    <value>T17 - Responsibility Determination</value>
                    <value>T18 - Equal Employment Opportunity (EEO) Compliance</value>
                    <value>T19 - No Proposal/Bid/Quote Correspondence</value>
                    <value>T20 - Unsuccessful Proposals/Bids/Quotes and Debriefings</value>        
                    <value>T21 - Mistakes/Protests</value>
                    <value>T22 - Late Bids/Proposals</value>
                    <value>T23 - Contract Actions</value>
                    <value>T24 - Legal and Management Reviews</value>        
                    <value>T25 - Subcontracting Plan and Approvals</value>
                    <value>T26 - Notice of Award/COR Delegation</value>
                    <value>T27 - Federal Procurement Data System</value>
                    <value>T28 - HSPD-12 Compliance</value>        
                    <value>T29 - Bonds and Insurance</value>
                    <value>T30 - Notice to Proceed and Orientation Conference</value>
                    <value>T31 - Payrolls and Labor Compliance</value>
                    <value>T32 - Payments, Inspection and Acceptance</value>        
                    <value>T33 - Quality Assurance/Warranties/Contractor Performance Information</value>
                    <value>T34 - Deliquencies/Disputes and Appeals/Terminations</value>
                    <value>T35 - General Correspondence</value>
                    <value>T36 - Contract Modifications</value>        
                    <value>T37 - Environmental Reporting</value>
                    <value>T38 - Contract Closeout Documentation</value>
                    <value>T39 - Progress Payment Record</value>
                </list>
            </parameter>
        </constraint>        
    </constraints>


    <types>
        <!-- Enterprise-wide generic document type -->
        <type name="edms:docbase">
            <title>EDMS Basic Content(Document)</title>
            <parent>cm:content</parent>
        </type>

        <type name="edms:doc">
            <title>EDMS Generic Content (Document)</title>
            <parent>edms:docbase</parent>
            <properties>
                <property name="edms:owner">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <multiple>false</multiple>
