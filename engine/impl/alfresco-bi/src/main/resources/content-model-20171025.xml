    <aspects>
        <aspect name="edms:policyaspect">
            <title>Policy Aspect</title>
            <properties>
                <property name="edms:policynumber">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <multiple>false</multiple>
                </property>
                <property name="edms:policysupersedes">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                </property>
                <property name="edms:policytype">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <multiple>false</multiple>
                    <constraints>
                        <constraint ref="edms:policytypelist" />
                    </constraints>
                </property>
            </properties>
        </aspect>

        <aspect name="edms:singeddateaspect">
            <title>Signed Date Aspect</title>
            <properties>
                <property name="edms:signeddate">
                    <type>d:date</type>
                    <mandatory>true</mandatory>
                    <multiple>false</multiple>
                </property>
                <property name="edms:expirationdate">
                    <type>d:date</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                </property>
            </properties>
        </aspect>

        <aspect name="edms:audienceaspect">
            <title>Audience Aspect</title>
            <properties>
                <property name="edms:audience">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <multiple>true</multiple>
                </property>
            </properties>
        </aspect>

        <aspect name="edms:publicationaspect">
            <title>Publication Aspect</title>
            <properties>
                <property name="edms:publicationtype">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <multiple>false</multiple>
                    <default></default>
                    <constraints>
                        <constraint ref="edms:pubtypelist" />
                    </constraints>
                </property>

                <property name="edms:subtitle">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                </property>

                <property name="edms:publicationdate">
                    <type>d:date</type>
                    <mandatory>true</mandatory>
                    <multiple>false</multiple>
                </property>
            </properties>
        </aspect>

        <aspect name="edms:contributoraspect">
            <title>Contributor Aspect</title>
            <properties>
                <property name="edms:contributor">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <multiple>true</multiple>
                </property>
            </properties>
        </aspect>

        <aspect name="edms:eventaspect">
            <title>Event Aspect</title>
            <properties>
                <property name="edms:startdate">
                    <type>d:date</type>
                    <mandatory>true</mandatory>
                    <multiple>false</multiple>
                </property>
            
                <property name="edms:enddate">
                    <type>d:date</type>
                    <mandatory>false</mandatory>
                    <multiple>false</multiple>
                </property>

                <property name="edms:eventtype">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <multiple>false</multiple>
                    <default></default>
                    <constraints>
                        <constraint ref="edms:eventlist" />
                    </constraints>
                </property>
            </properties>
        </aspect>

        <aspect name="edms:migrationaspect">
            <title>Migration Aspect</title>
            <properties>
                <property name="edms:migratedcreateddate">
                    <type>d:datetime</type>
                </property>
            
                <property name="edms:migratedcreator">
                    <type>d:text</type>
                </property>

                <property name="edms:migratedlastmodifieddate">
                    <type>d:datetime</type>
                </property>

                <property name="edms:migratedlastmodifiedby">
                    <type>d:text</type>
                </property>

                <property name="edms:migratedfolderpath">
                    <type>d:text</type>
                </property>
                <property name="edms:migrateddocid">
                    <type>d:text</type>
                </property>
                
