<?xml version="1.0" encoding="UTF-8"?>

<!-- Definition of new Model -->
<model name="edms:edmsmodel" xmlns="http://www.alfresco.org/model/dictionary/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.alfresco.org/model/dictionary/1.0 platform:/resource/caliente-engine-alfresco-bi/src/main/resources/alfresco-model.xsd">

    <!-- Optional meta-data about the model -->
    <description>EDMS Content Model</description>
    <author>Acuity Systems</author>
    <version>1.1.2</version>

    <!-- Imports are required to allow references to definitions in other models -->
    <imports>
        <!-- Import Alfresco Dictionary Definitions -->
        <import uri="http://www.alfresco.org/model/dictionary/1.0"
            prefix="d" />
        <!-- Import Alfresco Content Domain Model Definitions -->
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
    </imports>

    <!-- Introduction of new namespaces defined by this model -->
    <namespaces>
        <namespace uri="http://www.acuitys.com/model/content/1.0"
            prefix="edms" />
    </namespaces>

    <constraints>
        <constraint name="edms:pubtypelist" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value></value>
                    <value>Customer service and marketing material [14B035]</value>
                    <value>Brochure</value>
                    <value>Employee News [14A050a]</value>
                    <value>Handbook</value>
                    <value>Handout material [14B010]</value>
                    <value>Instructions</value>
                    <value>Motor vehicle correspondence [GRS 10/01]</value>
                    <value>Newsletter</value>
                    <value>Report file [14A060a]</value>
                    <value>Speech [14A010a(1)]</value>
                </list>
            </parameter>
        </constraint>
        
		<constraint name="edms:yearconstraint" type="MINMAX">
			<parameter name="minValue">
				<value>1800</value>
			</parameter>
			<parameter name="maxValue">
				<value>9999</value>
			</parameter>
		</constraint>
		
        <constraint name="edms:eventlist" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value></value>
                    <value>Business and supplier promotion [14B025]</value>
                    <value>Conference</value>
                    <value>Meeting</value>
                    <value>Presentation</value>
                </list>
            </parameter>
        </constraint>


        <constraint name="edms:disciplinelist" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>---</value>
                    <value>Architectural</value>
					<value>Civil</value>
					<value>Electrical</value>
					<value>Fire Protection</value>
					<value>Geotechnical</value>
					<value>Hazardous Materials</value>
					<value>Interiors</value>
					<value>Landscape</value>
					<value>Mechanical</value>
					<value>Plumbing</value>
					<value>Security</value>
					<value>Site</value>
					<value>Structural</value>
					<value>Survey/Mapping</value>
					<value>Telecommunications</value>
					<value>Fire Alarm</value>
					<value>Sprinkler</value>
					<value>Parking</value>
					<value>Energy Monitoring and Controls</value>
					<value>Other Disciplines</value>
                </list>
            </parameter>
        </constraint>

        <constraint name="edms:drawingsheettypelist" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>---</value>
                    <value>General</value>
                    <value>Plans</value>
                    <value>Elevations</value>
                    <value>Sections</value>
                    <value>Large-Scale Views</value>
                    <value>Details</value>
                    <value>Schedules and Diagrams</value>
                    <value>3D Representations</value>
                    <value>Other</value>
                </list>
            </parameter>
        </constraint>

        <constraint name="edms:policytypelist" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value></value>
                    <value>Agreement record [14B040]</value>
                    <value>Decision file [01B001a(1)]</value>
                    <value>Travel and transportation file [GRS 09/04a]</value>
                </list>
            </parameter>
        </constraint>
        
        <constraint name="edms:pbsregionlist" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>---</value>
                    <value>01 - Region 1</value>
                    <value>02 - Region 2</value>
                    <value>03 - Region 3</value>
                    <value>04 - Region 4</value>
                    <value>05 - Region 5</value>
                    <value>06 - Region 6</value>
                    <value>07 - Region 7</value>
                    <value>08 - Region 8</value>
                    <value>09 - Region 9</value>
                    <value>10 - Region 10</value>
                    <value>11 - Region 11</value>
                </list>
            </parameter>
        </constraint>
        
        <constraint name="edms:pbsbusinesslinelist" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>---</value>
                    <value>Real Estate</value>
                    <value>D&amp;C</value>
                    <value>Interior Design</value>
                    <value>Project Management</value>
                    <value>Portfolio</value>
                    <value>Service Centers</value>
                    <value>FM&amp;SP</value>
                    <value>Acquisition</value>
                </list>
            </parameter>
        </constraint>
        
        <constraint name="edms:buildingstatuslist" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>Active</value>
                    <value>Closed</value>
                    <value>Excess</value>        
                    <value>Pending</value>
                    <value>Decommissioned</value>
                    <value>Deleted</value>                                                    
                </list>
            </parameter>
        </constraint>        

        <constraint name="edms:oatypelist" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>---</value>
                    <value>Pro Forma - client signature required</value>
                    <value>Pro Forma - informational purposes only</value>
                    <value>Final/Administrative - informational purposes only</value>        
                </list>
            </parameter>
        </constraint>        

        <constraint name="edms:leasestatuslist" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>Approved</value>
                    <value>Expired</value>        
                    <value>Rejected</value>
                    <value>Under Review</value>
                </list>
            </parameter>
        </constraint>        
        
        <constraint name="edms:tablist" type="LIST">
            <parameter name="allowedValues">
