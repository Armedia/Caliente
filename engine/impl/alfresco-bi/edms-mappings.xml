<?xml version="1.0" encoding="UTF-8"?>
<attribute-mappings xmlns="http://www.armedia.com/ns/caliente/engine/alfresco-bi" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.armedia.com/ns/caliente/engine/alfresco-bi platform:/resource/caliente-engine-alfresco-bi/src/main/resources/alfresco-bi.xsd">

	<type-mappings name="edms:doc">
		<map tgt="cm:created">ucm:xGSADocumentCreateDate</map>
		<map tgt="cm:creator">ucm:xGSADocumentCreatedBy</map>
		<map tgt="cm:modified">ucm:xGSADocumentUpdateDate</map>
		<map tgt="cm:modifier">ucm:xGSADocumentUpdatedBy</map>
		<map tgt="cm:name">ucm:fFileName,ucm:dOriginalName</map>
		<map tgt="cm:title">ucm:dDocTitle</map>
		<map tgt="edms:buildingname" caseSensitive="false">bi:building_name</map>
		<map tgt="edms:buildingnumber2" caseSensitive="false">bi:building_number_2</map>
		<map tgt="edms:buildingnumber" caseSensitive="false">bi:bld_id_pk</map>
		<map tgt="edms:buildingstatus" caseSensitive="false">bi:building_status</map>
		<map tgt="edms:city" caseSensitive="false">bi:city</map>
		<map tgt="edms:county" caseSensitive="false">bi:county</map>
		<map tgt="edms:customercode">ucm:xGSAAgencyBureauCode</map>
		<map tgt="edms:customername" caseSensitive="false">bi:agency_bureau_name</map>
		<map tgt="edms:fieldofficename" caseSensitive="false">bi:field_office_desc</map>
		<map tgt="edms:fiscalyear">ucm:xGSAProcessedDate</map>
		<map tgt="edms:leasenumber">ucm:xGSALeaseNumber</map>
		<map tgt="edms:location">ucm:xGSARegionId</map>
		<map tgt="edms:owner">ucm:xGSADocumentCreatedBy</map>
		<map tgt="edms:pbsregion">ucm.xGSARegionId</map>
		<map tgt="edms:projectmanager" caseSensitive="false">bi:project_manager</map>
		<map tgt="edms:projectnumber">ucm:xGSAProjectNumber</map>
		<map tgt="edms:projecttitle" caseSensitive="false">bi:project_title</map>
		<map tgt="edms:servicecenter" caseSensitive="false">bi:service_center_name</map>
		<map tgt="edms:state" caseSensitive="false">bi:state_cd</map>
		<map tgt="edms:streetaddress2" caseSensitive="false">bi:address2</map>
		<map tgt="edms:streetaddress" caseSensitive="false">bi:street_address</map>
		<map tgt="edms:zipcode" caseSensitive="false">bi:zip_code</map>
		<set tgt="edms:function">Real Property Management</set>
		<set tgt="edms:organization">PR</set>
	</type-mappings>

	<type-mappings name="edms:leasing">
		<map tgt="edms:oanumber">ucm:xGSAOANumber</map>
		<map tgt="edms:appiandocid">ucm:xGSAAppianDocumentId</map>
		<map tgt="edms:appianuniversaldocid">ucm:xGSAAppianUniversalDocumentId</map>
	</type-mappings>

	<type-mappings name="edms:buildingproject">
		<set tgt="edms:businessline">Leasing</set>
	</type-mappings>

	<type-mappings name="edms:leasecontracting">
		<map tgt="edms:closeddate" caseSensitive="false">bi:expiration_dt</map>
		<set tgt="edms:contracttype">Lease</set>
		<map tgt="edms:projectmanager" caseSensitive="false">bi:project_manager</map>
		<map tgt="edms:tab">ucm:xGSAEleasePhaseName</map>
	</type-mappings>

</attribute-mappings>